{
  "name": "rolldown-enhance",
  "description": "Some plugins to make your rolldown experience better.",
  "version": "0.0.0",
  "type": "module",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aprosail/rolldown-enhance.git",
    "directory": "packages/rolldown-enhance"
  },
  "keywords": [
    "devtool",
    "plugins",
    "rolldown",
    "rolldown-enhance",
    "rolldown-plugins"
  ],
  "exports": {
    ".": {
      "types": "./out/index.d.ts",
      "import": "./out/index.js",
      "require": "./out/commonjs/index.js",
      "default": "./out/index.js"
    }
  },
  "scripts": {
    "prepublish": "tsc --noEmit && pnpm test && pnpm build",
    "build": "rolldown --config rolldown.config.ts",
    "test": "vitest run",
    "dev": "vitest",
    "changelog": "current-changelog"
  },
  "dependencies": {
    "tsconfig-aliases": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "catalog:dev",
    "current-changelog": "workspace:*",
    "rolldown": "catalog:dep",
    "rolldown-plugin-dts": "catalog:dev",
    "typescript": "catalog:dev",
    "vitest": "catalog:dev"
  }
}
