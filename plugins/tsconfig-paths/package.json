{
  "name": "rolldown-plugin-tsconfig-paths",
  "description": "A plugin to resolves paths from tsconfig for Rolldown.",
  "version": "1.0.1",
  "type": "module",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aprosail/rolldown-enhance.git",
    "directory": "plugins/tsconfig-paths"
  },
  "keywords": [
    "aliases",
    "rolldown",
    "rolldown-plugin",
    "tsconfig",
    "tsconfig-paths"
  ],
  "exports": {
    ".": {
      "types": "./out/index.d.ts",
      "import": "./out/index.js",
      "require": "./out/index.cjs",
      "default": "./out/index.js"
    }
  },
  "scripts": {
    "prepublish": "tsc --noEmit && pnpm build",
    "build": "rolldown --config rolldown.config.ts",
    "changelog": "current-changelog"
  },
  "dependencies": {
    "rolldown": "catalog:dep",
    "tsconfig-aliases": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "catalog:dev",
    "common-lib-tsconfig": "workspace:*",
    "current-changelog": "workspace:*",
    "rolldown-plugin-dts": "catalog:dev",
    "typescript": "catalog:dev"
  }
}
